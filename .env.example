# Domain Configuration (managed by shared infrastructure)
# NOTE: Must match the domain configured in shared Caddy reverse proxy
N8N_DOMAIN=n8n.example.com

# PostgreSQL Configuration
POSTGRES_DB=n8n_prod
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_this_admin_password

# n8n Database User (non-superuser for security)
N8N_DB_USER=n8n_app
N8N_DB_PASSWORD=change_this_n8n_password

# n8n Security Configuration (GENERATE THESE!)
N8N_ENCRYPTION_KEY=your-32-character-encryption-key-here
N8N_JWT_SECRET=your-jwt-secret-key-here

# n8n Feature Configuration
N8N_DIAGNOSTICS_ENABLED=false
N8N_PERSONALIZATION_ENABLED=false
N8N_LOG_LEVEL=info
GENERIC_TIMEZONE=UTC

# Optional: Basic Authentication
# N8N_BASIC_AUTH_ACTIVE=false
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=change_this_auth_password
